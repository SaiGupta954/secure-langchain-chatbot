# Server
PORT=8080
NODE_ENV=production
CORS_ORIGIN=*

# Frontend -> Backend shared secret for issuing tokens
APP_SECRET=change-me-app-secret

# JWT settings
JWT_SECRET=change-me-jwt-secret
JWT_EXPIRES=1h

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60

# OpenAI (server-side only)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini
